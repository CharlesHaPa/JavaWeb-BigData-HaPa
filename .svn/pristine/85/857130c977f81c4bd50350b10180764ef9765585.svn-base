package com.sw1408.po;

public class Seat {
	private int id;
	private String row_num;
	private String col_num;
	private String carriage;
	private int trainId;
	private String usable;
	private String type;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getRow_num() {
		return row_num;
	}
	public void setRow_num(String row_num) {
		this.row_num = row_num;
	}
	public String getCol_num() {
		return col_num;
	}
	public void setCol_num(String col_num) {
		this.col_num = col_num;
	}
	public String getCarriage() {
		return carriage;
	}
	public void setCarriage(String carriage) {
		this.carriage = carriage;
	}
	public int getTrainId() {
		return trainId;
	}
	public void setTrainId(int trainId) {
		this.trainId = trainId;
	}
	public String getUsable() {
		return usable;
	}
	public void setUsable(String usable) {
		this.usable = usable;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public Seat(int id, String row_num, String col_num, String carriage, int trainId, String usable, String type) {
		super();
		this.id = id;
		this.row_num = row_num;
		this.col_num = col_num;
		this.carriage = carriage;
		this.trainId = trainId;
		this.usable = usable;
		this.type = type;
	}
	public Seat() {
	}
	@Override
	public String toString() {
		return "Seat [id=" + id + ", row_num=" + row_num + ", col_num=" + col_num + ", carriage=" + carriage
				+ ", trainId=" + trainId + ", usable=" + usable + ", type=" + type + "]";
	}

}
