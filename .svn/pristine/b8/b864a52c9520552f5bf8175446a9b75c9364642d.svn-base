    margin-top: 10px;
    border-radius: 3px;
}
#searchBox ul{
	margin: 0;
}

.cursor{
	cursor: pointer;
}

#journeyType{
    float: left;
    width: 6%;
    text-align: center;
    margin-right: 0px;
}
#journeyType ul{
	padding: 0;
}
#journeyType li{
    height: 20px;
    padding: 2px;
	border-right: solid #298cce 1px;
}

#passengerType{
    float: left;
    width: 6%;
    text-align: center;
}

#passengerType ul{
	padding: 0;
}

#passengerType li{
	height: 20px;
    padding: 2px;
	border-right: solid #298cce 1px;
}

#placeSearch input{
    width: 122px;
    height: 30px;
    float: left;
}

#placeSearch label{
	display: block;
    width: 20%;
