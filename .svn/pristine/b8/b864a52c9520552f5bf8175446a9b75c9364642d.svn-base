}

#searchBox #placeSearch li{
	margin: 0 20px;
}
#placeSearch input{
	margin-left: 20px;
}
#searchBox #btn{
	width: 7%;
	height: 35px;
	float: right;
	margin-right: 8%;
	margin-top: 8px;
	height: 35px;
}
